				{
					"IRI": "Document/0003_chunk",
					"id": "0003",
					"content": "Geldend van 05-10-2022 t/m heden met terugwerkende kracht vanaf 01-10-2022",
					"@type": [
						"src:LeafElement"
					]
				},
				{
					"IRI": "Document/001t_intitule",
					"id": "001t",
					"children": [
						{
							"IRI": "Document/001t_intitule/0004_chunk",
							"id": "0004",
							"content": "Intitulé",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/0005_chunk",
							"id": "0005",
							"content": "Beleidsregel Individuele Inkomenstoeslag gemeente Utrecht",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/0006_chunk",
							"id": "0006",
							"content": "Burgemeester en wethouders van de gemeente Utrecht,",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/0007_chunk",
							"id": "0007",
							"content": "Gelet op de artikel 8, eerste lid, van de Participatiewet en artikel 36 van de Participatiewet en de Verordening Individuele Inkomenstoeslag Utrecht 2019;",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/0008_chunk",
							"id": "0008",
							"content": "Overwegende dat:",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/0009_chunk",
							"id": "0009",
							"content": "- Zij op grond van artikel 8, eerste lid, van de Participatiewet en artikel 36 van de Participatiewet en de Verordening Individuele Inkomenstoeslag Utrecht 2019 bevoegd zijn, op verzoek van een persoon van 21 jaar of ouder doch jonger dan de pensioengerechtigde leeftijd, een individuele inkomenstoeslag te verlenen;",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/000a_chunk",
							"id": "000a",
							"content": "- De persoon recht heeft op een individuele inkomenstoeslag wanneer er sprake is van een langdurig laag inkomen en geen in aanmerking te nemen vermogen als bedoeld in artikel 34 van de wet en de persoon geen uitzicht heeft op inkomensverbetering;",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/000b_chunk",
							"id": "000b",
							"content": "- De Participatiewet het college verplicht om bij de beoordeling van de aanspraak op een individuele inkomenstoeslag de omstandigheden en inspanningen van de persoon mede in overweging te nemen alsmede invulling geeft aan de wijze waarop het inkomen als bedoeld in artikel 36 van de wet wordt vastgesteld;",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/000c_chunk",
							"id": "000c",
							"content": "Besluiten vast te stellen de volgende Beleidsregel Individuele Inkomenstoeslag gemeente Utrecht",
							"@type": [
								"src:LeafElement"
							]
						},
						{
							"IRI": "Document/001t_intitule/000d_chunk",
							"id": "000d",
							"content": "Hoofdstuk 1 Algemene bepalingen",
							"@type": [
								"src:LeafElement"
							]
						}
					],
